import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
		int n = 4;
		int m = 4;
		
		for (int i = 0; i<n ; i++) {
		    for (int s=0;s<=i-1;s++){
		        System.out.print(" ");
		    }
		    for(int j=0;j<m;j++){
		        System.out.print("#");
		    }
		    System.out.println();
	 }
}
}
